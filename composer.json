{
    "name": "mistralys/series-manager",
    "description": "Series Manager for tracking TV series watching.",
    "license": "MIT",
    "type": "project",
    "homepage": "https://github.com/Mistralys/series-manager",
    "authors": [
        {
            "name": "Sebastian Mordziol",
            "email": "s.mordziol@mistralys.eu",
            "role": "Project Lead, Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "assets\/classes\/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/classes"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Mistralys/thetvdb2.git"
        }
    ],
    "require": {
        "mistralys\/application-utils": "^2.3.2",
        "mistralys\/application-localization": "^1.4.1",
        "mistralys/changelog-parser": "^1.0.1",
        "mistralys/html_quickform2": "^2.2.0",
        "adrenth/thetvdb2": ">=6.1.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.6.9",
        "phpstan/phpstan": ">=1.10.20",
        "roave/security-advisories": "dev-latest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
